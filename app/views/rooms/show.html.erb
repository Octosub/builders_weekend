<h3>Create room</h3>

<h1><%= @room.name %></h1>

<h2>Description:</h2>
<p><%= @room.description %></p>

<% @room.photos.each do |photo| %>
  <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
<% end %>

<%= video_tag @room.video_filepath.split("/")[-1], controls: true %>
<%= @room.video_filepath %>
